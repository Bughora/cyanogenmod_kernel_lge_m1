/dts-v1/;

#include "msm8909-e1q_crk_us.dtsi"
#include "../../qcom/msm8909-pm8916.dtsi"
#include "msm8909-pm8916-e1q_crk_us.dtsi"

/ {
	model = "Qualcomm Technologies, Inc. MSM8909 PM8916 E1Q CRK US";
	compatible = "qcom,msm8909";
	qcom,board-id= <0x064 0>, /* evb1 */
	               <0x164 0>, /* evb2 */
	               <0x264 2>, /* rev_0 */
	               <0x364 2>; /* rev_a */
};

&sdhc_2 {
	cd-gpios = <&msm_gpio 38 0x1>;
};

&sdc2_cd_on {
	bias-pull-up;
};
